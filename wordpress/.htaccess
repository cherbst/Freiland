
# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /freiland/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /freiland/index.php [L]
</IfModule>

# END WordPress
RedirectMatch permanent /freiland/$ /freiland/category/events
